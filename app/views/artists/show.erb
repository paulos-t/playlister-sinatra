<h2><%= @artist.name %></h2>

<br>
<h3>Songs</h3>

<% @artist.songs.each do |song| %>
    <p> <a href="/songs/<%= song.slug %>"><%= song.name %></a></p>
<% end %>
<br>
<br>

<h3>Genres</h3>

<%# binding.pry %>
<% @artist.genres.each do |genre| %>
    <p> <a href="/genres/<%= genre.slug %>"> <%= genre.name.capitalize %> </a> </p>
<% end %>